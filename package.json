{
  "version": "0.1.3",
  "name": "@mist3rbru/my-cli",
  "access": "public",
  "private": false,
  "main": "dist/index.js",
  "bin": {
    "my-cli": "./dist/index.js",
    "my": "./dist/index.js"
  },
  "author": "Mist3rBru",
  "license": "MIT",
  "homepage": "https://github.com/Mist3rBru/my-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mist3rBru/my-cli-api"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx src/index.ts",
    "my": "run-s \"dev {@}\" --",
    "lint": "tsc && prettier --write .",
    "build": "tsup src",
    "test": "jest --no-cache",
    "test:w": "npm test -- --watch",
    "test:ci": "npm test -- --coverage --silent",
    "ci": "run-s lint build test",
    "publish": "changeset publish",
    "release": "run-s ci publish"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "@mist3rbru/create-ts-api": "^0.1.3",
    "axios": "^1.3.4",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@swc/core": "^1.3.46",
    "@swc/jest": "^0.2.24",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3"
  }
}
