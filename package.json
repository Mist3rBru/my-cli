{
  "version": "0.2.19",
  "name": "@mist3rbru/my-cli",
  "access": "public",
  "private": false,
  "author": {
    "name": "Mist3rBru",
    "git": "https://github.com/Mist3rBru"
  },
  "license": "MIT",
  "homepage": "https://github.com/Mist3rBru/my-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mist3rBru/my-cli"
  },
  "bin": {
    "my-cli": "./bin/index.cjs",
    "my": "./bin/index.cjs"
  },
  "files": [
    "bin",
    "public"
  ],
  "keywords": [
    "cli",
    "prompt",
    "api",
    "template",
    "git"
  ],
  "type": "module",
  "scripts": {
    "my": "cross-env NODE_ENV=development tsx src/index.ts",
    "start": "node bin/index.cjs",
    "lint": "run-s lint:tsc lint:prettier lint:eslint",
    "lint:tsc": "tsc",
    "lint:prettier": "prettier --write .",
    "lint:eslint": "eslint --fix \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "build": "unbuild",
    "clean": "rimraf bin coverage",
    "test": "jest",
    "test:w": "jest --watch",
    "test:ci": "jest --coverage --silent",
    "docs": "tsx scripts/docs",
    "ci": "cross-env NODE_ENV=test run-s docs lint build test",
    "publish": "changeset publish",
    "release": "run-s ci publish"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@clack/core": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "@mist3rbru/create-ts-api": "^0.1.3",
    "axios": "^1.6.7",
    "open": "^10.0.3",
    "pacote": "^17.0.6",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@swc/core": "^1.4.0",
    "@swc/jest": "^0.2.36",
    "@total-typescript/ts-reset": "^0.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/pacote": "^11.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-mist3rbru": "^0.3.5",
    "jest": "^29.7.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  }
}
