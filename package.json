{
  "version": "0.2.5",
  "name": "@mist3rbru/my-cli",
  "access": "public",
  "private": false,
  "author": {
    "name": "Mist3rBru",
    "git": "https://github.com/Mist3rBru"
  },
  "license": "MIT",
  "homepage": "https://github.com/Mist3rBru/my-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mist3rBru/my-cli"
  },
  "bin": {
    "my-cli": "./dist/index.cjs",
    "my": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "prompt",
    "api",
    "template",
    "git"
  ],
  "scripts": {
    "my": "cross-env NODE_ENV=development tsx src/index.ts",
    "start": "node dist/index.cjs",
    "lint": "tsc --noEmit && prettier --write .",
    "build": "unbuild",
    "test": "jest --no-cache",
    "test:w": "npm test -- --watch",
    "test:ci": "npm test -- --coverage --silent",
    "docs": "tsx scripts/docs",
    "ci": "npm run my run lint build test docs -- --force",
    "publish": "changeset publish",
    "release": "npm run my run ci publish -- --force"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "@mist3rbru/create-ts-api": "^0.1.3",
    "axios": "^1.4.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@swc/core": "^1.3.58",
    "@swc/jest": "^0.2.26",
    "@total-typescript/ts-reset": "^0.4.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1"
  }
}
